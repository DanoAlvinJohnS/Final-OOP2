    backdrop-filter: blur(10px);
    box-shadow: 0px 4px 12px rgba(0, 0, 0, 0.4);
    transition: all 0.25s ease-in-out;
}

QPushButton:hover {
    background-color: qlineargradient(
        spread:pad,
        x1:0, y1:0, x2:1, y2:1,
        stop:0 rgba(65, 72, 95, 200),
        stop:0.5 rgba(55, 63, 85, 200),
        stop:1 rgba(45, 52, 75, 200)
    );
    box-shadow: 0px 6px 16px rgba(79, 86, 255, 0.35);
    transform: scale(1.04);
}

QPushButton:pressed {
    background-color: qlineargradient(
        spread:pad,
        x1:0, y1:0, x2:1, y2:1,
        stop:0 rgba(30, 35, 55, 220),
        stop:0.5 rgba(25, 30, 50, 220),
        stop:1 rgba(20, 25, 40, 220)
    );
    box-shadow: 0px 3px 8px rgba(0, 0, 0, 0.3);
    transform: scale(0.97);
}

